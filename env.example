# Sample configuration for darkweb666
# Copy this file to .env and adjust values as needed.

# Tor SOCKS proxy URL used by monitor_onion.py and future modules
TOR_PROXY=socks5h://127.0.0.1:9050

# Redis connection string (optional features)
REDIS_URL=redis://localhost:6379/0

# Database URL for storing scan results
DATABASE_URL=sqlite:///db.sqlite3

# Webhook URL for sending alerts
WEBHOOK_URL=https://example.com/webhook

# API token for integrations
API_TOKEN=change_me

# Logging level
LOG_LEVEL=INFO
